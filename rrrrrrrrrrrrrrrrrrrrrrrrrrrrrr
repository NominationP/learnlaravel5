http://127.0.0.1:1024

php artisan make:auth


php artisan make:model Article
php artisan make:migration create_article_table

php artisan make:model Comment -m

php artisan migrate


php artisan make:seeder ArticleSeeder
composer dump-autoload
php artisan db:seed


